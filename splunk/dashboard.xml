  <row>
    <panel>
      <title>XSS Attempts(POST PAYLOADS)</title>
      <table>
        <search>
          <query>index=* sourcetype=vulnweb_post
| where like(BODY, "%&lt;script&gt;%")
       OR like(BODY, "%onerror%")
       OR like(BODY, "%javascript:%")
| table _time IP PATH BODY
| sort - _time</query>
          <earliest>0</earliest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>SQL Injection Attempts</title>
      <table>
        <search>
          <query>index=* sourcetype=vulnweb_post
| where like(BODY, "%' or 1=1%")
       OR like(BODY, "%' or '1'='1%")
       OR like(BODY, "%--%")
       OR like(BODY, "%UNION SELECT%")
| table _time IP PATH BODY
| sort - _time</query>
          <earliest>0</earliest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">100</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">none</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
</dashboard>
